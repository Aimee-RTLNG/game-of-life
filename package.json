{
  "private": true,
  "devDependencies": {
    "fs-extra": "9.1.0",
    "noliter": "1.0.0",
    "parcel-bundler": "1.12.5",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  },
  "scripts": {
    "fix": "npx prettier --write ./docs/patterns",
    "test": "prettier --check ./docs/patterns",
    "start": "parcel src/index.html -d docs",
    "prebuild": "node ./scripts/prebuild.js",
    "build": "parcel build src/index.html -d docs --public-url ./"
  }
}
